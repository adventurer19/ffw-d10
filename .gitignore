# gitignore template for Drupal 8 projects
#
# earlier versions of Drupal are tracked in `community/PHP/`

# Ignore configuration files that may contain sensitive information
# drupal-dir/web/sites/*/*settings*.php
# drupal-dir/web/sites/*/*services*.yml

# # Ignore paths that may contain user-generated content
# drupal-dir/web/sites/*/files
# drupal-dir/web/sites/*/public
# drupal-dir/web/sites/*/private
# drupal-dir/web/sites/*/files-public
# drupal-dir/web/sites/*/files-private

# # Ignore paths that may contain temporary files
# drupal-dir/web/sites/*/translations
# drupal-dir/web/sites/*/tmp
# drupal-dir/web/sites/*/cache

# Ignore testing related files
drupal-dir/web/sites/simpletest

.vagrant

# Ignore drupal core (if not versioning drupal sources)
/core
/modules/README.txt
/profiles/README.txt
drupal-dir/web/sites/README.txt
drupal-dir/web/sites/exampledrupal-dir/web.sites.php
drupal-dir/web/sites/example.settings.local.php
drupal-dir/web/sites/development.services.yml
/themes/README.txt
/vendor
/.csslintrc
/.editorconfig
/.eslintignore
/.eslintrc.json
/.gitattributes
/.htaccess
/autoload.php
/composer.json
/composer.lock
/example.gitignore
/index.php
/LICENSE.txt
/README.txt
/robots.txt
/update.php
/web.config
